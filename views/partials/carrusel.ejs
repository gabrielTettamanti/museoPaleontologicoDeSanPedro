<section class="carousel">

    <% news.forEach(noticia => { %>
        <div class="slide">
            <%- include('./carruselCard', {noticia}) %> 
        </div>
    <% }) %>

    <div class="back carousel-button">
        <i class="fa-solid fa-arrow-left"></i>
    </div>

    <div class="forward carousel-button">
        <i class="fa-solid fa-arrow-right"></i>
    </div>
</section>