<section class="carousel">

    <% sponsors.forEach(sponsor => { %>
        <div class="slide-carousel" id="sponsor-slide">
            <%- include('./sponsorCard', { sponsor }) %> 
        </div>
    <% }) %>

    <div class="back carousel-button" id="back-sponsor">
        <i class="fa-solid fa-arrow-left"></i>
    </div>

    <div class="forward carousel-button" id="forward-sponsor">
        <i class="fa-solid fa-arrow-right"></i>
    </div>

</section>